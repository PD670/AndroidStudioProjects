<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
 android:background="@color/black"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
android:weightSum="2"
        android:layout_height="200dp">
        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:srcCompat="@drawable/img_31" />

        </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="match_parent"
        android:layout_marginStart="4dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        app:srcCompat="@drawable/img_32" />
</LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:weightSum="2"
        android:layout_height="200dp">

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/img_33" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginStart="4dp"
            app:srcCompat="@drawable/img_34" />
    </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="horizontal"
    android:layout_height="200dp">
    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/img_35" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="100dp"
            android:orientation="vertical"
            android:weightSum="2"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                app:srcCompat="@drawable/img_36" />

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/img_37" />

        </LinearLayout>
<LinearLayout
    android:layout_width="100dp"
    android:orientation="vertical"
    android:weightSum="2"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginEnd="2dp"
        android:layout_marginStart="4dp"
        app:srcCompat="@drawable/img_38" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="match_parent"
        android:layout_marginStart="4dp"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/img_39" />

</LinearLayout>
    </LinearLayout>
</LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="horizontal"
    android:weightSum="4"
    android:layout_height="100dp">

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        app:srcCompat="@drawable/img_40" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        app:srcCompat="@drawable/img_41" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        app:srcCompat="@drawable/img_42" />

    <Button
        android:id="@+id/button2"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="back" />

</LinearLayout>
</LinearLayout>

